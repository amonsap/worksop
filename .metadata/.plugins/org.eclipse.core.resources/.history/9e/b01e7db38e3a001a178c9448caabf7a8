package th.ac.kku.member;

public class main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
	}
	public void SilverUpToGold(int order, int price,int number) {
		if((order >= 4)&&(price >=1000)&&(number<=200)) {
			System.out.println("Gold");
		}
		System.out.println("Silve");
	}

}
